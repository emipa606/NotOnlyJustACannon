<Defs>
  <!-- 實心彈 -->
  <ThingDef ParentName="MakeableShellBase">
    <defName>CannonBall_Solidshot</defName>
    <label>Cannon Solid Round</label>
    <description>A solid Metal Round</description>
    <recipeMaker>
      <workSpeedStat>SmithingSpeed</workSpeedStat>
      <effectWorking>Cook</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workAmount>200</workAmount>
      <targetCountAdjustment>5</targetCountAdjustment>
      <workSkill>Crafting</workSkill>
      <recipeUsers>
        <li>FueledSmithy</li>
        <li>ElectricSmithy</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>2</Crafting>
      </skillRequirements>
    </recipeMaker>
    <techLevel>Medieval</techLevel>
    <thingCategories>
      <li>CannonShells</li>
    </thingCategories>
    <tradeTags>
      <li>MortarShell</li>
    </tradeTags>
    <graphicData>
      <texPath>Things/Item/CannonBall_Solid</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>0.25</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <explosiveExpandPerStackcount>0.05</explosiveExpandPerStackcount>
        <startWickHitPointsPercent>0.4</startWickHitPointsPercent>
        <wickTicks>
          <min>30</min>
          <max>60</max>
        </wickTicks>
      </li>
    </comps>
    <projectileWhenLoaded>Bullet_CannonBall_Solidshot</projectileWhenLoaded>
    <costList>
      <Steel>10</Steel>
    </costList>
  </ThingDef>
  <ThingDef ParentName="BaseBullet">
    <defName>Bullet_CannonBall_Solidshot</defName>
    <label>CannonBall</label>
    <graphicData>
      <texPath>Things/Projectile/Projectile_Solid</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
    </graphicData>
    <thingClass>Projectile_Explosive</thingClass>
    <projectile>
      <damageDef>SolidShell</damageDef>
